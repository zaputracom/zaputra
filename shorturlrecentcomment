$(document).ready(function(){$("#notifikasi").click(function(){$("#two-tab-recent-comments").toggleClass("aktif")})});$(document).ready(function(){$("#notifikasi").click(function(){$(".tab-comment").toggleClass("aktif")})});$(document).ready(function(){$("#two-tab-recent-comments").click(function(){$("#two-tab-recent-comments").removeClass("aktif")})});$(document).ready(function(){$("#two-tab-recent-comments").click(function(){$(".tab-comment").removeClass("aktif")})});function loadscript(a){var b=document.createElement("script");b.setAttribute("type","text/javascript");b.setAttribute("src",a);document.getElementsByTagName("head")[0].appendChild(b)}
function urltinyfyprompt_callback(a){waitElm&&(waitElm.style.display="none");var b="Short URL";a.title&&""!=a.title?b+=' for "'+a.title+'"':a.longurl&&""!=a.longurl&&(b+=" for "+a.longurl);a.tinyurl&&""!=a.tinyurl?prompt(b+":",a.tinyurl):alert("Could not get short URL, try again later."+(a.error&&""!=a.error?"nError: "+a.error:""))}
function shortenUrl(a,b,c){(waitElm=a.parentNode.nextSibling)&&(waitElm.style.display="inline");loadscript("https://urltinyfy.appspot.com/"+c+"?unify=1&url="+encodeURIComponent(b)+"&callback=urltinyfyprompt_callback")}var waitElm=null;
